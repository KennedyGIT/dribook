<!DOCTYPE html>
<html lang="en">
  <!--Head Start Section-->
  <%-include('layouts/head'); -%>
   <!--Head End Section-->


  <body>
    <!-- start cssload-loader -->
    <div class="preloader" id="preloader">
      <div class="loader">
        <svg class="spinner" viewBox="0 0 50 50">
          <circle
            class="path"
            cx="25"
            cy="25"
            r="20"
            fill="none"
            stroke-width="5"
          ></circle>
        </svg>
      </div>
    </div>
    <!-- end cssload-loader -->

    <!-- ================================
            START HEADER AREA
================================= -->
  <%-include('layouts/appointmentheader'); -%>
    <!-- ================================
         END HEADER AREA
================================= -->


 <!-- ================================
       START BREADCRUMB AREA
================================= -->
<section class="breadcrumb-area bread-bg-8">
    <div class="breadcrumb-wrap">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6">
                    <div class="breadcrumb-content">
                        <div class="section-heading">
                            <h2 class="sec__title text-white">Examinations</h2>
                        </div>
                    </div><!-- end breadcrumb-content -->
                </div><!-- end col-lg-6 -->
                <div class="col-lg-6">
                    <div class="breadcrumb-list text-right">
                        <ul class="list-items">
                            <li>Exam Bookings</li>
                        </ul>
                    </div><!-- end breadcrumb-list -->
                </div><!-- end col-lg-6 -->
            </div><!-- end row -->
        </div><!-- end container -->
    </div><!-- end breadcrumb-wrap -->
    <div class="bread-svg-box">
        <svg class="bread-svg" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 10" preserveAspectRatio="none"><polygon points="100 0 50 10 0 0 0 10 100 10"></polygon></svg>
    </div><!-- end bread-svg -->
</section>
 <!-- ================================
       END BREADCRUMB AREA
================================= -->


<!-- ================================
    START CARD AREA
================================= -->
<section class="card-area section--padding">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="filter-wrap margin-bottom-30px">
                    <div class="filter-bar d-flex align-items-center justify-content-between">
                        <div class="filter-bar-filter d-flex flex-wrap align-items-center">
                            <div class="filter-option">
                                <h3 class="title font-size-16">Filter by:</h3>
                            </div>
                            <div class="filter-option">
                                <div class="dropdown dropdown-contain">
                                    <a class="dropdown-toggle dropdown-btn dropdown--btn" href="#" role="button" data-toggle="dropdown">
                                        Test Type
                                    </a>
                                    <div class="dropdown-menu dropdown-menu-wrap">
                                        <div class="dropdown-item">
                                            <div class="checkbox-wrap">
                                                <div class="custom-checkbox">
                                                    <input type="checkbox" value="G" id="catChb1">
                                                    <label for="catChb1">G</label>
                                                </div>
                                                <div class="custom-checkbox">
                                                    <input type="checkbox" value="G2" id="catChb2">
                                                    <label for="catChb2">G2</label>
                                                </div>
                                            </div>
                                        </div><!-- end dropdown-item -->
                                    </div><!-- end dropdown-menu -->
                                </div><!-- end dropdown -->
                            </div>
                        </div><!-- end filter-bar-filter -->
                    </div><!-- end filter-bar -->
                </div><!-- end filter-wrap -->
            </div><!-- end col-lg-12 -->
        </div><!-- end row -->
        <div class="row">
            <div class="col-lg-12">
                <div id="userListContainer">
                    <% userList.forEach(user => { %>
                        <div class="card-item car-card card-item-list">
                          <div class="card-body">
                            <div class="card-rating">
                              <span class="badge text-white">Result</span>
                              <span class="review__text"><%= user.examStatus || 'Unknown or Pending' %></span> <!-- Provide a default value if examStatus is null or undefined -->
                            </div>
                            <div class="card-price d-flex align-items-center justify-content-between">
                              <p>
                                <span class="price__num"><%= user.firstName %> <%= user.lastName %></span>
                                <% if (user.examType) { %>
                                  <span class="price__text"><strong>Test Type : </strong><%= user.examType %></span>
                                  <span class="price__license"><strong>License Number : </strong><%= user.licenseNo %></span>
                                <% } else { %>
                                  <span class="price__text">N/A</span> <!-- Provide a default value if appointment fields are null or undefined -->
                                <% } %>
                              </p>
                              <a href="/" class="btn-text" data-toggle="modal" data-target="#userdetailsform">See details<i class="la la-angle-right"></i></a>
                            </div>
                          </div>
                        </div><!-- end card-item -->
                      <% }); %>
                  </div>
            </div><!-- end col-lg-12 -->
        </div><!-- end row -->
    </div><!-- end container -->
</section><!-- end card-area -->
<!-- ================================
    END CARD AREA
================================= -->

   
  

    <!-- ================================
       START FOOTER AREA
================================= -->
    <%-include('layouts/footer'); -%>
    <!-- end footer-area -->
    <!-- ================================
       END FOOTER AREA
================================= -->

    <!-- start back-to-top -->
    <div id="back-to-top">
      <i class="la la-angle-up" title="Go top"></i>
    </div>
    <!-- end back-to-top -->
    <!-- end modal-shared -->
    <%-include('layouts/userdetails'); -%>
    <!-- end modal-popup -->



    <!-- Template JS Files -->
    <%-include('layouts/scripts'); -%>
    
  </body>
</html>
