
<!DOCTYPE html>
<html lang="en">
  <!--Head Start Section-->
  <%-include('layouts/head'); -%>
   <!--Head End Section-->


  <body>
    <!-- start cssload-loader -->
    <div class="preloader" id="preloader">
      <div class="loader">
        <svg class="spinner" viewBox="0 0 50 50">
          <circle
            class="path"
            cx="25"
            cy="25"
            r="20"
            fill="none"
            stroke-width="5"
          ></circle>
        </svg>
      </div>
    </div>
    <!-- end cssload-loader -->

    <!-- ================================
            START HEADER AREA
================================= -->
  <%-include('layouts/unauthorizedheader');-%>
    <!-- ================================
         END HEADER AREA
================================= -->
<section class="breadcrumb-area bread-bg-9">
    <div class="breadcrumb-wrap">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6">
                    <div class="breadcrumb-content">
                        <div class="section-heading">
                            <h2 class="sec__title text-white">Login</h2>
                        </div>
                    </div><!-- end breadcrumb-content -->
                </div><!-- end col-lg-6 -->
                <div class="col-lg-6">
                </div><!-- end col-lg-6 -->
            </div><!-- end row -->
        </div><!-- end container -->
    </div><!-- end breadcrumb-wrap -->
    
</section>


<section class="contact-area padding-top-100px padding-bottom-70px">
    <div class="container">
        <div class="row">
            <div class="col-lg-7 mx-auto">
                <div class="form-box">
                    <div class="form-title-wrap">
                        <h3 class="title">Welcome. Login to continue</h3>
                    </div><!-- form-title-wrap -->
                    <div class="form-content ">
                        <div class="contact-form-action">
                            <form method="post" id = loginForm>
                                <div class="input-box">
                                    
                                    <label class="label-text" id="labelwarning"></label>
                                    <div class="form-group select-contain w-100 ">
                                        <div class="dropdown bootstrap-select select-contain-select ">
                                            <select class="select-contain-select" tabindex="-98" id="loginusertype" name="userType">
                                                <option value="" selected>Select User Type</option>
                                                <option value="Examiner">Examiner</option>
                                                <option value="Driver">Driver</option>
                                                <option value="Admin">Admin</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <span class="la la-user form-icon"></span>
                                        <input class="form-control" type="text" name="username" id = "username" placeholder="Enter username">
                                    </div>
                                    
                                    <div class="form-group">
                                        <span class="la la-lock form-icon"></span>
                                        <input class="form-control" type="password" name="password" id = "password" placeholder="password">
                                    </div>
                                </div>
                                <div class="btn-box">
                                    <button type="button" id = "login"class="theme-btn">Login</button>
                                </div>
                            </form>
                        </div><!-- end contact-form-action -->
                    </div><!-- end form-content -->
                </div><!-- end form-box -->
            </div><!-- end col-lg-8 -->
        </div><!-- end row -->
    </div><!-- end container -->
</section>


  

<div class="section-block"></div>

   
  

    <!-- ================================
       START FOOTER AREA
================================= -->
<%-include('layouts/footer'); -%>
    <!-- end footer-area -->
    <!-- ================================
       END FOOTER AREA
================================= -->

    <!-- start back-to-top -->
    <div id="back-to-top">
      <i class="la la-angle-up" title="Go top"></i>
    </div>
    <!-- end back-to-top -->

    <!-- end modal-shared -->
    <%-include('layouts/signup'); -%>
    <!-- end modal-popup -->

    <!-- Template JS Files -->
    <%-include('layouts/scripts'); -%>
    
  </body>
</html>
