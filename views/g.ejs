<!DOCTYPE html>
<html lang="en">
  <!--Head Start Section-->
  <%-include('layouts/head'); -%>
   <!--Head End Section-->


  <body>
    <!-- start cssload-loader -->
    <div class="preloader" id="preloader">
      <div class="loader">
        <svg class="spinner" viewBox="0 0 50 50">
          <circle
            class="path"
            cx="25"
            cy="25"
            r="20"
            fill="none"
            stroke-width="5"
          ></circle>
        </svg>
      </div>
    </div>
    <!-- end cssload-loader -->

    <!-- ================================
            START HEADER AREA
================================= -->
  <%-include('layouts/header'); -%>
    <!-- ================================
         END HEADER AREA
================================= -->

<section class="breadcrumb-area bread-bg-8">
  <div class="breadcrumb-wrap">
      <div class="container">
          <div class="row align-items-center">
              <div class="col-lg-12">
                  <div class="search-result-content">
                      <div class="section-heading">
                          <h2 class="sec__title text-white">Car License Search Result</h2>
                      </div>
                      <div class="search-fields-container margin-top-30px">
                          <div class="contact-form-action">
                              <form action="/g2" class="row">
                                  <div class="col-lg-12 col-sm-6 pr-0">
                                      <div class="input-box">
                                          <label class="label-text">Car License Number</label>
                                          <div class="form-group">
                                              <span class="la la-search form-icon"></span>
                                              <input class="form-control" id = "licenseNo" type="text" placeholder="Car License Number">
                                          </div>
                                      </div>
                                  </div><!-- end col-lg-4 -->

                              </form>
                          </div><!-- end contact-form-action -->
                          
                          <div class="btn-box pt-3">
                              <button class="theme-btn" type="submit" id="searchbtn" type="button">Search Now</button>
                          </div>
                      </div><!-- end search-fields-container -->
                  </div><!-- end search-result-content -->
              </div><!-- end col-lg-12 -->
          </div><!-- end row -->
      </div><!-- end container -->
  </div><!-- end breadcrumb-wrap -->
  <div class="bread-svg-box">
      <svg class="bread-svg" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 10" preserveAspectRatio="none"><polygon points="100 0 50 10 0 0 0 10 100 10"></polygon></svg>
  </div><!-- end bread-svg -->
</section>

<section class="booking-area padding-top-100px padding-bottom-70px">
  <div class="container">
      <div class="row">
          <div class="col-lg-12">
              <div class="form-box">
                  <div class="form-title-wrap">
                      <h3 class="title">Your Personal Information</h3>
                  </div><!-- form-title-wrap -->
                  <div class="form-content ">
                      <div class="contact-form-action">
                          <form id="personalDetailsForm">
                              <div class="row">
                                  <div class="col-lg-6 responsive-column">
                                      <div class="input-box">
                                          <label class="label-text">First Name</label>
                                          <div class="form-group">
                                              <span class="la la-user form-icon"></span>
                                              <input class="form-control" type="text" id="firstName" name="firstName" placeholder="First name" disabled required>
                                              <span></span>
                                          </div>
                                      </div>
                                  </div><!-- end col-lg-6 -->
                                  <div class="col-lg-6 responsive-column">
                                      <div class="input-box">
                                          <label class="label-text">Last Name</label>
                                          <div class="form-group">
                                              <span class="la la-user form-icon"></span>
                                              <input class="form-control" id="lastName" type="text" name="lastName" placeholder="Last name" disabled required>
                                              <span></span>
                                          </div>
                                      </div>
                                  </div><!-- end col-lg-6 -->
                                  <div class="col-lg-6 responsive-column">
                                      <div class="input-box">
                                          <label class="label-text">License Number</label>
                                          <div class="form-group">
                                              <span class="la la-envelope-o form-icon"></span>
                                              <input class="form-control" id="license" type="text" name="licenseNo" placeholder="License" disabled  required  pattern="[a-z0-9]+" required>
                                              <span></span>
                                          </div>
                                      </div>
                                  </div><!-- end col-lg-6 -->
                                  <div class="col-lg-6 responsive-column">
                                      <div class="input-box">
                                          <label class="label-text">Age</label>
                                          <div class="form-group">
                                              <span class="la la-user form-icon"></span>
                                              <input class="form-control" id="age" type="number" name="age" placeholder="Age" disabled required>
                                              <span></span>
                                          </div>
                                      </div>
                                  </div><!-- end col-lg-6 -->

                                  <div class="col-lg-6 responsive-column">
                                    <div class="input-box">
                                        <label class="label-text">DOB</label>
                                        <div class="form-group">
                                            <span class="la la-pencil form-icon"></span>
                                            <input class="form-control" type="date" id="dob" name="dob" placeholder="Date of birth" disabled required>
                                            <span></span>
                                        </div>
                                    </div>
                                </div><!-- end col-lg-6 -->
                              </div>
                          </form>
                      </div><!-- end contact-form-action -->
                  </div><!-- end form-content -->
              </div><!-- end form-box -->
              <div class="form-box">
                  <div class="form-title-wrap">
                      <h3 class="title">Your Car Information</h3>
                  </div><!-- form-title-wrap -->
                  <div class="form-content">
                      
                      <div class="tab-content">
                          <div class="tab-pane fade show active" id="credit-card" role="tabpanel" aria-labelledby="credit-card-tab">
                              <div class="contact-form-action">
                                  <form id="carForm">
                                      <div class="row">
                                          <div class="col-lg-6 responsive-column">
                                              <div class="input-box">
                                                  <label class="label-text">Card Make</label>
                                                  <div class="form-group">
                                                      <span class="la la-car form-icon"></span>
                                                      <input class="form-control" type="text" id="make" name="make" placeholder="Make" required>
                                                      <span></span>
                                                  </div>
                                              </div>
                                          </div><!-- end col-lg-6 -->
                                          <div class="col-lg-6 responsive-column">
                                              <div class="input-box">
                                                  <label class="label-text">Model</label>
                                                  <div class="form-group">
                                                      <span class="la la-car form-icon"></span>
                                                      <input class="form-control" type="text" id="model" name="model" placeholder="Model" required>
                                                      <span></span>
                                                  </div>
                                              </div>
                                          </div><!-- end col-lg-6 -->
                                          <div class="col-lg-6">
                                              <div class="row">
                                                  <div class="col-lg-6 responsive-column">
                                                      <div class="input-box">
                                                          <label class="label-text">Year</label>
                                                          <div class="form-group">
                                                              <span class="la la-pencil form-icon"></span>
                                                              <input class="form-control" type="text" id="carYear" name="year" placeholder="YYYY" required>
                                                              <span></span>
                                                          </div>
                                                      </div>
                                                  </div>
                                                  <div class="col-lg-6 responsive-column">
                                                      <div class="input-box">
                                                          <label class="label-text">Plat Number</label>
                                                          <div class="form-group">
                                                              <span class="la la-car form-icon"></span>
                                                              <input class="form-control" type="text" name="platNo" id="platNumber" placeholder="Plat Number" required>
                                                              <span></span>
                                                          </div>
                                                      </div>
                                                  </div>
                                              </div>
                                          </div><!-- end col-lg-6 -->
                                          
                                          
                                          <div class="col-lg-12">
                                              <div class="btn-box">
                                                  <button type= "submit" class="theme-btn" id ="submitbtn">Update Booking</button>
                                              </div>
                                          </div><!-- end col-lg-12 -->
                                      </div>
                                  </form>
                              </div><!-- end contact-form-action -->
                          </div><!-- end tab-pane-->
                      </div><!-- end tab-content -->
                  </div><!-- end form-content -->
              </div><!-- end form-box -->
          </div><!-- end col-lg-8 -->
          
      </div><!-- end row -->
  </div><!-- end container -->
</section><!-- end booking-area -->

  

    <div class="section-block"></div>

   
  

    <!-- ================================
       START FOOTER AREA
================================= -->
<%-include('layouts/footer'); -%>
    <!-- end footer-area -->
    <!-- ================================
       END FOOTER AREA
================================= -->

    <!-- start back-to-top -->
    <div id="back-to-top">
      <i class="la la-angle-up" title="Go top"></i>
    </div>
    <!-- end back-to-top -->

    <!-- end modal-shared -->
    <div class="modal-popup">
      <div
        class="modal fade"
        id="signupPopupForm"
        tabindex="-1"
        role="dialog"
        aria-hidden="true"
      >
        <div class="modal-dialog modal-dialog-centered" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <div>
                <h5 class="modal-title title" id="exampleModalLongTitle">
                  Sign Up
                </h5>
                <p class="font-size-14">Hello! Welcome Create a New Account</p>
              </div>
              <button
                type="button"
                class="close"
                data-dismiss="modal"
                aria-label="Close"
              >
                <span aria-hidden="true" class="la la-close"></span>
              </button>
            </div>
            <div class="modal-body">
              <div class="contact-form-action">
                <form method="post">
                  <div class="input-box">
                    <label class="label-text">Username</label>
                    <div class="form-group">
                      <span class="la la-user form-icon"></span>
                      <input
                        class="form-control"
                        type="text"
                        name="text"
                        placeholder="Type your username"
                      />
                    </div>
                  </div>
                  <!-- end input-box -->
                  <div class="input-box">
                    <label class="label-text">Email Address</label>
                    <div class="form-group">
                      <span class="la la-envelope form-icon"></span>
                      <input
                        class="form-control"
                        type="text"
                        name="text"
                        placeholder="Type your email"
                      />
                    </div>
                  </div>
                  <!-- end input-box -->
                  <div class="input-box">
                    <label class="label-text">Password</label>
                    <div class="form-group">
                      <span class="la la-lock form-icon"></span>
                      <input
                        class="form-control"
                        type="text"
                        name="text"
                        placeholder="Type password"
                      />
                    </div>
                  </div>
                  <!-- end input-box -->
                  <div class="input-box">
                    <label class="label-text">Repeat Password</label>
                    <div class="form-group">
                      <span class="la la-lock form-icon"></span>
                      <input
                        class="form-control"
                        type="text"
                        name="text"
                        placeholder="Type again password"
                      />
                    </div>
                  </div>
                  <!-- end input-box -->
                  <div class="btn-box pt-3 pb-4">
                    <button type="button" class="theme-btn w-100">
                      Register Account
                    </button>
                  </div>
                  <div class="action-box text-center">
                    <p class="font-size-14">Or Sign up Using</p>
                    <ul class="social-profile py-3">
                      <li>
                        <a href="#" class="bg-5 text-white"
                          ><i class="lab la-facebook-f"></i
                        ></a>
                      </li>
                      <li>
                        <a href="#" class="bg-6 text-white"
                          ><i class="lab la-twitter"></i
                        ></a>
                      </li>
                      <li>
                        <a href="#" class="bg-7 text-white"
                          ><i class="lab la-instagram"></i
                        ></a>
                      </li>
                      <li>
                        <a href="#" class="bg-5 text-white"
                          ><i class="lab la-linkedin-in"></i
                        ></a>
                      </li>
                    </ul>
                  </div>
                </form>
              </div>
              <!-- end contact-form-action -->
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- end modal-popup -->

    <!-- end modal-shared -->
    <%-include('layouts/login'); -%>
    <!-- end modal-popup -->

    <!-- Template JS Files -->
    <%-include('layouts/scripts'); -%>
    
  </body>
</html>
