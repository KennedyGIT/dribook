<!DOCTYPE html>
<html lang="en">
  <!--Head Start Section-->
  <%-include('layouts/head'); -%>
   <!--Head End Section-->


  <body>
    <!-- start cssload-loader -->
    <div class="preloader" id="preloader">
      <div class="loader">
        <svg class="spinner" viewBox="0 0 50 50">
          <circle
            class="path"
            cx="25"
            cy="25"
            r="20"
            fill="none"
            stroke-width="5"
          ></circle>
        </svg>
      </div>
    </div>
    <!-- end cssload-loader -->

    <!-- ================================
            START HEADER AREA
================================= -->
<%-include('layouts/authorizedheader'); -%>
    <!-- ================================
         END HEADER AREA
================================= -->

<section class="booking-area padding-top-100px padding-bottom-70px">
  <div class="container">
      <div class="row">
          <div class="col-lg-12">
              <div class="form-box">
                  <div class="form-title-wrap">
                      <h3 class="title">Your Personal Information</h3>
                  </div><!-- form-title-wrap -->
                  <div class="form-content ">
                      <div class="contact-form-action">
                          <form id="personalDetailsForm">
                              <div class="row">
                                  <div class="col-lg-6 responsive-column">
                                      <div class="input-box">
                                          <label class="label-text">First Name</label>
                                          <div class="form-group">
                                              <span class="la la-user form-icon"></span>
                                              <input class="form-control" type="text" id="firstName" name="firstName" placeholder="First name" required>
                                              <span></span>
                                          </div>
                                      </div>
                                  </div><!-- end col-lg-6 -->
                                  <div class="col-lg-6 responsive-column">
                                      <div class="input-box">
                                          <label class="label-text">Last Name</label>
                                          <div class="form-group">
                                              <span class="la la-user form-icon"></span>
                                              <input class="form-control" id="lastName" type="text" name="lastName" placeholder="Last name"  required>
                                              <span></span>
                                          </div>
                                      </div>
                                  </div><!-- end col-lg-6 -->
                                  <div class="col-lg-6 responsive-column">
                                      <div class="input-box">
                                          <label class="label-text">License Number</label>
                                          <div class="form-group">
                                              <span class="la la-envelope-o form-icon"></span>
                                              <input class="form-control" id="license" type="text" name="licenseNo" placeholder="License"  required  pattern="[a-z0-9]+" required>
                                              <span></span>
                                          </div>
                                      </div>
                                  </div><!-- end col-lg-6 -->
                                  <div class="col-lg-6 responsive-column">
                                      <div class="input-box">
                                          <label class="label-text">Age</label>
                                          <div class="form-group">
                                              <span class="la la-user form-icon"></span>
                                              <input class="form-control" id="age" type="number" name="age" placeholder="Age" required>
                                              <span></span>
                                          </div>
                                      </div>
                                  </div><!-- end col-lg-6 -->

                                  <div class="col-lg-6 responsive-column">
                                    <div class="input-box">
                                        <label class="label-text">DOB</label>
                                        <div class="form-group">
                                            <span class="la la-pencil form-icon"></span>
                                            <input class="form-control" type="date" id="dob" name="dob" placeholder="Date of birth"  required>
                                            <span></span>
                                        </div>
                                    </div>
                                </div><!-- end col-lg-6 -->
                              </div>
                          </form>
                      </div><!-- end contact-form-action -->
                  </div><!-- end form-content -->
              </div><!-- end form-box -->
              <div class="form-box">
                  <div class="form-title-wrap">
                      <h3 class="title">Your Car Information</h3>
                  </div><!-- form-title-wrap -->
                  <div class="form-content">
                      
                      <div class="tab-content">
                          <div class="tab-pane fade show active" id="credit-card" role="tabpanel" aria-labelledby="credit-card-tab">
                              <div class="contact-form-action">
                                  <form id="carForm">
                                      <div class="row">
                                          <div class="col-lg-6 responsive-column">
                                              <div class="input-box">
                                                  <label class="label-text">Card Make</label>
                                                  <div class="form-group">
                                                      <span class="la la-car form-icon"></span>
                                                      <input class="form-control" type="text" id="make" name="make" placeholder="Make" required>
                                                      <span></span>
                                                  </div>
                                              </div>
                                          </div><!-- end col-lg-6 -->
                                          <div class="col-lg-6 responsive-column">
                                              <div class="input-box">
                                                  <label class="label-text">Model</label>
                                                  <div class="form-group">
                                                      <span class="la la-car form-icon"></span>
                                                      <input class="form-control" type="text" id="model" name="model" placeholder="Model" required>
                                                      <span></span>
                                                  </div>
                                              </div>
                                          </div><!-- end col-lg-6 -->
                                          <div class="col-lg-6">
                                              <div class="row">
                                                  <div class="col-lg-6 responsive-column">
                                                      <div class="input-box">
                                                          <label class="label-text">Year</label>
                                                          <div class="form-group">
                                                              <span class="la la-pencil form-icon"></span>
                                                              <input class="form-control" type="text" id="carYear" name="year" placeholder="YYYY" required>
                                                              <span></span>
                                                          </div>
                                                      </div>
                                                  </div>
                                                  <div class="col-lg-6 responsive-column">
                                                      <div class="input-box">
                                                          <label class="label-text">Plat Number</label>
                                                          <div class="form-group">
                                                              <span class="la la-car form-icon"></span>
                                                              <input class="form-control" type="text" name="platNo" id="platNumber" placeholder="Plat Number" required>
                                                              <span></span>
                                                          </div>
                                                      </div>
                                                  </div>
                                              </div>
                                          </div><!-- end col-lg-6 -->
                                          
                                          
                                          <div class="col-lg-12">
                                              <div class="btn-box">
                                                  <button  class="theme-btn" id ="submit-btn">Confirm Booking</button>
                                              </div>
                                          </div><!-- end col-lg-12 -->
                                      </div>
                                  </form>
                              </div><!-- end contact-form-action -->
                          </div><!-- end tab-pane-->
                      </div><!-- end tab-content -->
                  </div><!-- end form-content -->
              </div><!-- end form-box -->
          </div><!-- end col-lg-8 -->
          
      </div><!-- end row -->
  </div><!-- end container -->
</section><!-- end booking-area -->

  

  <div class="section-block"></div>

   
  

    <!-- ================================
       START FOOTER AREA
================================= -->
<%-include('layouts/footer'); -%>
    <!-- end footer-area -->
    <!-- ================================
       END FOOTER AREA
================================= -->

    <!-- start back-to-top -->
    <div id="back-to-top">
      <i class="la la-angle-up" title="Go top"></i>
    </div>
    <!-- end back-to-top -->

    
    <%-include('layouts/signup'); -%>
    <!-- end modal-popup -->

    <!-- end modal-shared -->
    <%-include('layouts/login'); -%>
    <!-- end modal-popup -->

    <!-- Template JS Files -->
    <%-include('layouts/scripts'); -%>
    
  </body>
</html>
